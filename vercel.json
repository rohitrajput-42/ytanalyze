{
    "builds": [
      {
        "src": "monetize/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.10.0"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "ui"
        }
      }
    ],
    "routes": [
      {
        "src": "/ui/(.*)",
        "dest": "/ui/$1"
      },
      {
        "src": "/(.*)",
        "dest": "monetize/wsgi.py"
      }
    ]
  }
  