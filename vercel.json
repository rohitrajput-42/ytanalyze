{
  "builds": [
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build"
    },
    {
      "src": "manage.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "manage.py"
    }
  ]
}
